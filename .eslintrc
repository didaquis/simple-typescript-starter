{
	"root": true,
	"env": {
		"jest": true,
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@didaquis/no-lenght",
		"@typescript-eslint",
		"prettier"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"rules": {
		"@didaquis/no-lenght/no-lenght": "error",
		"no-console": "warn",
		"prettier/prettier": "error",
		"curly": "error",
		"no-return-await": "error"
	}
}